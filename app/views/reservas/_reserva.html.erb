<tr <%= reserva.fecha.wday == 0 ? '' : 'class=table-danger' %> >
	<th scope="col"><%= reserva.id %></th>
	<th><%= reserva.fecha %></th>
	<th><%= link_to reserva.prop.nombre, reserva.prop %></th>
	<th><%= link_to reserva.usuario.email, reserva.usuario %></th>
	<th><%= button_to "Eliminar", reserva, delete_reserva_path(@reserva.id) </th>
</tr>
<%= form_for @reserva do |d| %>

	<% lista_usuario = options_from_collection_for_select(Usuario.all, 'id', 'email') %>
	<% lista_prop = options_from_collection_for_select(Prop.all, 'id', 'nombre') %>

	<%= d.select :usuario_id, lista_usuario %><br><br>
	<%= d.select :prop_id, lista_prop %><br><br>

	<%= d.date_field :fecha %><br><br>

	<%= d.submit t('.save') %>
<% end %>

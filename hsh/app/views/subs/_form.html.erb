<%= form_for @sub do |d| %>

	<% lista_prop = options_from_collection_for_select(Prop.all, 'id', 'nombre') %>
	<span>Fecha de inicio: <span>
	<%= d.date_field :fecha_inicio, required: true%><br><br>
	<span>Fecha de fin: <span>
	<%= d.date_field :fecha_fin, required: true %><br><br>
	<%= d.select :prop_id, lista_prop%><br><br>
	<%= d.submit t('.save') %>
<% end %>
